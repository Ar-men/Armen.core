use Exclus::Exclus;
use Jargon::Process;

#md_## Les méthodes
#md_

#md_### _run_process()
#md_
sub _run_process {
    Jargon::Process->new->process;
}

###----+----+----+----+----+----+----+----+----+----+----+----+----+----+----+----+----+----+----+----+----+----+----###
exit _run_process;
